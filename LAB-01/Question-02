#include<stdio.h>
int main(){
    float width;
    float height;
    printf("Enter Width of a Rectangle: \n");
    scanf("%f", &width);

    printf("Enter Height of a Rectangle: \n");
    scanf("%f", &height);

    float area = width*height;
    float perimeter = 2*(width + height);

    printf("Area of Rectangle is: %f \n", area);
    printf("Perimeter of Reactangle is: %f", perimeter);

    return 0;
}
